{
  "name": "redux-rags",
  "version": "1.0.0",
  "description": "Redux Reducers, Actions, and Getters. Simplified!",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d build --copy-files",
    "lint": "eslint src/**",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "flow": "flow",
    "test": "npm run jest",
    "jest": "./node_modules/.bin/jest --maxWorkers=2",
    "jest:watch": "npm run jest -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeSignal/redux-rags.git"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "jest": {
    "testMatch": [
      "<rootDir>/**/?(*.)+(test).js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/test/jest.transform.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "modulePaths": [
      "<rootDir>/"
    ]
  },
  "keywords": [
    "redux"
  ],
  "author": "CodeSignal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeSignal/redux-rags/issues"
  },
  "homepage": "https://github.com/CodeSignal/redux-rags#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.8.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.86.0",
    "jest": "^23.6.0",
    "redux": "^4.0.1"
  }
}
