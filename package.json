{
  "name": "redux-rags",
  "version": "1.2.1",
  "description": "Redux Reducers, Actions, and Getters. Simplified!",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build --copy-files",
    "lint": "eslint src/**",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "flow": "flow",
    "test": "npm run jest -- --coverage --coverageReporters=text-lcov | ./node_modules/coveralls/bin/coveralls.js",
    "jest": "./node_modules/.bin/jest --maxWorkers=2",
    "jest:watch": "npm run jest -- --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeSignal/redux-rags.git"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "redux"
  ],
  "author": "CodeSignal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeSignal/redux-rags/issues"
  },
  "homepage": "https://github.com/CodeSignal/redux-rags#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-watch": "^5.1.2",
    "flow-bin": "^0.102.0-rc",
    "jest": "^24.8.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2"
  }
}
